{
  "agent": {
    "skipBootstrap": true
  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/.openclaw/workspace",
      "model": "anthropic/claude-sonnet-4-5",
      "thinkingDefault": "low",
      "heartbeat": {
        "every": "24h"
      },
      "compaction": {
        "enabled": true
      }
    }
  },
  "tools": {
    "profile": "review",
